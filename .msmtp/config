account mike@padiernos.me
host mail.padiernos.me
port 465
from mike@padiernos.me
user mike@padiernos.me
passwordeval "pass mail/mike@padiernos.me"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/mikepadiernos/.config/msmtp/msmtp.log
tls_starttls off

account mike.padiernos@gmail.com
host smtp.gmail.com
port 587
from mike.padiernos@gmail.com
user mike.padiernos@gmail.com
passwordeval "pass mail/mike.padiernos@gmail.com"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/mikepadiernos/.config/msmtp/msmtp.log
# tls_starttls


